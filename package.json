{
  "name": "g-factor",
  "version": "1.1.0",
  "description": "Utility to get files & folders size",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "tsc -p tsconfig.dev.json --watch",
    "build": "yarn run format &&  tsc -p tsconfig.prod.json",
    "format": "prettier --write 'src/**/*.*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pranshuchittora/g-factor.git"
  },
  "author": "Pranshu Chittora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pranshuchittora/g-factor/issues"
  },
  "homepage": "https://github.com/pranshuchittora/g-factor#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "fs": "^0.0.1-security"
  },
  "keywords": [
    "utility",
    "file size",
    "folder size",
    "fs wrapper"
  ]
}
